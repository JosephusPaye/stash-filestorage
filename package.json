{
  "name": "@josephuspaye/stash-filestorage",
  "version": "0.0.0",
  "description": "Simple file-based cache storage for @josephuspaye/stash",
  "main": "dist/FileStorage.js",
  "types": "dist/FileStorage.d.ts",
  "repository": "https://github.com/JosephusPaye/stash-filestorage.git",
  "author": "Josephus Paye II",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "uvu -r esm tests/",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.js\" \"*.{js,json,md}\"",
    "format:check": "prettier --list-different \"src/**/*.ts\" \"tests/**/*.js\" \"*.{js,json,md}\""
  },
  "dependencies": {
    "devalue": "^2.0.1"
  },
  "peerDependencies": {
    "@josephuspaye/stash": "^0.2.0"
  },
  "devDependencies": {
    "@josephuspaye/stash": "^0.2.0",
    "@types/node": "^14.14.6",
    "esm": "^3.2.25",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3",
    "uvu": "^0.3.4"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true
  }
}
